{
  "name": "realworld",
  "version": "0.0.0",
  "description": "Имплементация REST API для блога на Express и MongoDB",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && node ./dist/index.js",
    "build": "npm run clean && npm run compile",
    "clean": "rimraf dist",
    "compile": "tsc -p tsconfig.json",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "prettier --write src/**/*.{js,ts} --log-level silent",
    "ts": "ts-node",
    "prepare": "husky install",
    "mock:server": "json-server ./mocks/mock-server-data.json --host 127.0.0.1 --port 3123"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pavellbor/realworld.git"
  },
  "keywords": [
    "realworld",
    "node",
    "express",
    "mongodb",
    "blog"
  ],
  "author": "Pavel Borisov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pavellbor/realworld/issues"
  },
  "homepage": "https://github.com/pavellbor/realworld#readme",
  "devDependencies": {
    "@types/node": "^20.10.5",
    "husky": "^8.0.0",
    "json-server": "^0.17.4",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "type": "module",
  "engines": {
    "node": "^18.0.0",
    "npm": ">=8"
  },
  "dependencies": {
    "dayjs": "^1.11.10",
    "got": "^13.0.0",
    "pino": "^8.17.2"
  }
}
